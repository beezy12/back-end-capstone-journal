{
  "name": "back-end-capstone-journal",
  "version": "1.0.0",
  "description": "A nice journalling app I built as my back-end capstone using the MEAN stack.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development nodemon server.js",
    "start": "NODE_ENV=production node server.js",
    "bower": "bower",
    "postinstall": "node node_modules/bower/bin/bower install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beezy12/back-end-capstone-journal.git"
  },
  "eslintConfig": {
    "env": {
      "mocha": true,
      "node": true,
      "es6": true,
      "browser": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "res": true,
      "app": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 0,
      "no-undef": 0,
      "no-extra-semi": 0,
      "no-redeclare": 0,
      "comma-dangle": 0,
      "no-mixed-spaces-and-tabs": 0,
      "no-extra-boolean-cast": 0,
      "no-empty": 0
    }
  },
  "keywords": [],
  "author": "Brian Stumbaugh <brian.stumbaugh@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beezy12/back-end-capstone-journal/issues"
  },
  "homepage": "https://github.com/beezy12/back-end-capstone-journal#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "browser-sync": "^2.11.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-jade": "^1.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-ruby-sass": "^2.0.6",
    "gulp-sass": "^2.2.0",
    "gulp-uglify": "^1.5.3",
    "nodemon": "^1.9.1"
  },
  "dependencies": {
    "bcrypt": "^0.8.5",
    "body-parser": "^1.15.0",
    "bower": "^1.7.7",
    "connect-flash": "^0.1.1",
    "connect-redis": "^3.0.2",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "jade": "^1.11.0",
    "method-override": "^2.3.5",
    "mongoose": "^4.4.6",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "webpack": "^1.12.14"
  }
}

